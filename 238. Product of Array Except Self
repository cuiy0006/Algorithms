class Solution:
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        output = nums[:]
        for i in range(1, len(nums)-1):
            output[i] = output[i] * output[i-1]
        right = 1
        for i in range(len(nums)-1, -1, -1):
            if i == 0:
                output[i] = right
            else:
                output[i] = output[i-1] * right
            right = right * nums[i]
        return output
