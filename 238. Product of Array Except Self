class Solution(object):
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        res = []
        tmp = 1
        for num in nums:
            tmp = tmp * num
            res.append(tmp)
        
        tmp = 1
        for i in range(len(nums)-1, -1, -1):
            if i != 0:
                res[i] = tmp * res[i-1]
            else:
                res[i] = tmp
            tmp *= nums[i]
        return res
