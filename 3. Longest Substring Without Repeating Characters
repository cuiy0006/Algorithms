class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        maxlen = 0
        i, j = 0, 0
        dic = {}
        cnt = 0
        while j < len(s):
            if s[j] in dic and dic[s[j]] > 0:
                cnt += 1
            dic[s[j]] = dic.get(s[j], 0) + 1
            maxlen = max(maxlen, j-i)
            while cnt > 0:
                dic[s[i]] -= 1
                if dic[s[i]] == 1:
                    cnt -= 1
                i += 1
            j += 1
        return max(maxlen, j-i)
