class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        a = []
        dic = {'(':')', '{':'}', '[':']'}
        for c in s:
            if c in dic:
                a.append(c)
            else:
                if len(a) == 0:
                    return False
                else:
                    if dic[a[-1]] == c:
                        a.pop()
                    else:
                        return False
        return len(a) == 0
