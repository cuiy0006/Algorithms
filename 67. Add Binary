class Solution(object):
    def addBinary(self, a, b):
        """
        :type a: str
        :type b: str
        :rtype: str
        """
        res = ''
        a = a[::-1]
        b = b[::-1]
        tmp = 0
        i = 0
        while i < len(a) and i < len(b):
            digit = int(a[i]) + int(b[i]) + tmp
            res = str(digit % 2) + res
            tmp = digit // 2
            i += 1
        while i < len(a):
            digit = int(a[i]) + tmp
            res = str(digit % 2) + res
            tmp = digit // 2
            i += 1
        while i < len(b):
            digit = int(b[i]) + tmp
            res = str(digit % 2) + res
            tmp = digit // 2
            i += 1
        if tmp == 1:
            res = '1' + res
        return res
