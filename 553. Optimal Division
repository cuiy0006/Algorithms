class Solution(object):
    def optimalDivision(self, nums):
        """
        :type nums: List[int]
        :rtype: str
        """
        if len(nums) == 1:
            return str(nums[0])
        if len(nums) == 2:
            return str(nums[0]) + '/' + str(nums[1])
        s = str(nums[0]) + '/(' + str(nums[1])
        for i in range(2, len(nums)):
            s += '/' + str(nums[i])
        return s + ')'
