# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        if head == None:
            return None
        first = head
        second = head.next
        third = head.next
        first.next = None
        while second != None:
            third = second.next
            second.next = first
            first = second
            second = third
        return first
