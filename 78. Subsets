class Solution(object):
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        res = []
        self.helper(nums, 0, [], res)
        return res
        
        
    def helper(self, nums, idx, curr, res):
        res.append(curr[:])
        
        for i in range(idx, len(nums)):
            curr.append(nums[i])
            self.helper(nums, i + 1, curr, res)
            curr.pop()
