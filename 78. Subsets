class Solution(object):
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        def helper(lst, res, index):
            res.append(lst[:])
            for i in range(index, len(nums)):
                num = nums[i]
                lst.append(num)
                helper(lst, res, i+1)
                lst.pop()
        res = []
        helper([], res, 0)
        return res
