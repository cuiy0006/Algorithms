class Solution(object):
    def rotate(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: void Do not return anything, modify matrix in-place instead.
        """
        n = len(matrix)
        left = 0
        right = n - 1
        for i in range(n//2):
            for j in range(left, right):
                x = i
                y = j
                tmp = matrix[x][y]
                for k in range(4):
                    matrix[y][n-x-1],tmp = tmp,matrix[y][n-x-1]
                    x, y = y, n-x-1
            left += 1
            right -= 1
