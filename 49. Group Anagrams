class Solution(object):
    def groupAnagrams(self, strs):
        """
        :type strs: List[str]
        :rtype: List[List[str]]
        """
        dic = {}
        res = []
        for s in strs:
            arr = [0] * 26
            for c in s:
                arr[ord(c) - ord('a')] += 1
            tp = tuple(arr)
            if tp in dic:
                dic[tp].append(s)
            else:
                dic[tp] = [s]
        return list(dic.values())
